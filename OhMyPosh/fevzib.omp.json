{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "display_mode": "environment",
            "fetch_virtual_env": true,
            "folder_name_fallback": false
          },
          "style": "plain",
          "template": "({{ .Venv }}) ",
          "foreground": "#00FF00",
          "type": "python"
        },
        {
          "style": "plain",
          "template": "[{{ .HostName }}] ",
          "foreground": "#32b058",
          "type": "text"
        },
        {
          "style": "plain",
          "template": "{{ .UserName }}",
          "foreground": "#E36464",
          "type": "session"
        },
        {
          "style": "plain",
          "template": " âžœ ",
          "foreground": "#62ED8B",
          "type": "text"
        },
        {
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }}",
          "foreground": "#56B6C2",
          "type": "path"
        },
        {
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": " <#5FAAE8>git(</>{{ .HEAD }}<#5FAAE8>)</>",
          "foreground": "#D0666F",
          "type": "git"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
